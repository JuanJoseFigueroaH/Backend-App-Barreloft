datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Users {
  id            Int            @id @default(autoincrement())
  name          String
  email         String
  telefono      String
  password      String
  session       Int            @default(0)
  deleted       Boolean        @default(false)
  created_at    DateTime       @default(now())
  updated_at    DateTime       @updatedAt
  deleted_at    DateTime?

  @@map(name: "users")
}
